#ifndef INCLUDE_DEFINE_GSXRAM_H_
#define INCLUDE_DEFINE_GSXRAM_H_

#define Cpu1toCpu2_1					((volatile float *)(0xC000))
#define Cpu1toCpu2_2					((volatile float *)(0xD000))
#define Cpu1toCpu2_3					((volatile float *)(0xE000))
#define Cpu1toCpu2_4					((volatile float *)(0xF000))
#define Cpu1toCpu2_5					((volatile float *)(0x10000))
#define Cpu1toCpu2_6					((volatile float *)(0x11000))
#define Cpu1toCpu2_7					((volatile float *)(0x12000))
#define Cpu1toCpu2_8					((volatile float *)(0x13000))
#define Cpu2toCpu1_1					((volatile float *)(0x14000))
#define Cpu2toCpu1_2					((volatile float *)(0x15000))
#define Cpu2toCpu1_3					((volatile float *)(0x16000))
#define Cpu2toCpu1_4					((volatile float *)(0x17000))
#define Cpu2toCpu1_5					((volatile float *)(0x18000))
#define Cpu2toCpu1_6					((volatile float *)(0x19000))
#define Cpu2toCpu1_7					((volatile float *)(0x1A000))
#define Cpu2toCpu1_8					((volatile float *)(0x1B000))

#endif /* INCLUDE_DEFINE_GSXRAM_H_ */
